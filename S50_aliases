#!/bin/zsh

autoload zmv

alias su='sudo -s'

alias cless='less -R'
alias devlog='less -R log/development.log'
alias devtail='tail -n 1000 log/development.log | less -R'

alias du="du -hc"
alias dus="du -hcs"
alias df="df -h"

alias ls="ls -G"
alias la="ls -Ga"
alias ll="ls -Ghl"
alias lla="ls -Ghla"
alias lal="ls -Ghla"

# Talia aliases
alias talia='./talia.sh shell'
alias nscr='screen -h 5000 -S'
# alias rm='rm -i'


